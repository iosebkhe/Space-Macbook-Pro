{
  "name": "space",
  "version": "1.0.0",
  "description": "Space landing page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass main.scss style.css -w",
    "compile:sass": "node-sass main.scss style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.comp.css -o style.prefix.css",
    "build:css": "npm-run-all compile:sass prefix:css"
  },
  "author": "iosebkhe",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^7.0.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12"
  }
}
